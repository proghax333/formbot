{
  "name": "formbot",
  "version": "1.0.0",
  "description": "Form auto-fill and scrape",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --ignore *.json --exec babel-node --trace-warnings ./src/index.js",
    "build": "babel -d ./build ./src -s",
    "clean": "rm -rf build && mkdir build",
    "clean-build": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
    "serve": "npm run clean-build && node ./build/WebApi.js"
  },
  "keywords": [
    "google",
    "form",
    "autofill",
    "fill"
  ],
  "author": "Atmanand Nagpure",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "@babel/node": "^7.13.13",
    "console-log-html": "^2.0.2",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "got": "^11.8.2",
    "jsdom": "^16.5.3",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "require": "^2.4.20"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/register": "^7.13.16",
    "babel-plugin-import-directory": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
